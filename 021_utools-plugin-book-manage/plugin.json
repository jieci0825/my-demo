{
    "pluginName": "书签管理",
    "description": "一款用来管理浏览器书签的工具",
    "author": "Your Name",
    "homepage": "https://github.com/yourusername/book-manage",
    "version": "0.0.1",
    "logo": "logo.png",
    "platform": ["win32", "darwin", "linux"],
    "features": [
        {
            "code": "bookmark_manager",
            "explain": "书签管理器",
            "cmds": [
                {
                    "type": "over",
                    "label": "书签管理",
                    "match": {
                        "app": ["书签管理", "bookmark", "书签"]
                    }
                }
            ]
        },
        {
            "code": "search_bookmark",
            "explain": "搜索书签",
            "cmds": [
                {
                    "type": "input",
                    "label": "搜索书签",
                    "match": {
                        "placeholder": "搜索书签..."
                    }
                }
            ]
        }
    ]
}
