  "paths": {
    "/api/abc/": {
      "get": {
        "summary": "获取 abc 列表",
        "description": "获取 abc 列表的描述\n\n**备注:** 获取 abc 列表的备注\n\n**适用平台:** admin, h5",
        "operationId": "list",
        "tags": [
          "列表"
        ],
        "parameters": [
          {
            "name": "page",
            "in": "query",
            "required": true,
            "schema": {
              "type": "integer",
              "description": "页码",
              "minimum": 1,
              "maximum": 9007199254740991
            },
            "description": "页码"
          },
          {
            "name": "pageSize",
            "in": "query",
            "required": true,
            "schema": {
              "type": "integer",
              "description": "每页条数",
              "minimum": 1,
              "maximum": 100
            },
            "description": "每页条数"
          }
        ],
        "responses": {
          "200": {
            "description": "成功响应",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "required": [
                    "success",
                    "errorCode",
                    "message",
                    "data"
                  ],
                  "properties": {
                    "success": {
                      "type": "boolean",
                      "description": "操作是否成功",
                      "example": true
                    },
                    "errorCode": {
                      "type": "integer",
                      "description": "错误码（成功时为 0）",
                      "example": 0
                    },
                    "message": {
                      "type": "string",
                      "description": "成功消息",
                      "example": "操作成功"
                    },
                    "data": {
                      "type": "object",
                      "properties": {
                        "list": {
                          "type": "array",
                          "items": {
                            "type": "string"
                          }
                        },
                        "total": {
                          "type": "number"
                        }
                      },
                      "additionalProperties": false,
                      "required": [
                        "list",
                        "total"
                      ],
                      "$schema": "http://json-schema.org/draft-07/schema#"
                    },
                    "timestamp": {
                      "type": "string",
                      "format": "date-time",
                      "description": "时间戳（ISO 8601 格式）",
                      "example": "2025-11-20T06:11:20.162Z"
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "请求参数错误（errorCode: 3001）",
