{
    "manifest_version": 3,
    "name": "阅读时长",
    "version": "1.0",
    "description": "为 Chrome 扩展文档文章添加阅读时间",
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    },
    "content_scripts": [
        {
            "js": ["js/content.js"],
            "matches": ["https://developer.mozilla.org/zh-CN/*"]
        }
    ]
}
